{
  "name": "blog-back",
  "version": "1.1.0",
  "description": "Blog Back-end part done on Node.js",
  "main": "app.js",
  "scripts": {
    "dev": "env-cmd -f dev.env ts-node-dev --respawn --transpile-only app.ts",
    "start": "node node_modules/puppeteer/install.js & node dist/app.js",
    "build": "tsc -p .",
    "lint": "eslint . --fix",
    "test:jest": "jest --config ./jest.config.json",
    "test:jest-watch": "jest --config ./jest.config.json --watch",
    "dummy-users": "node dist/config/dummy-users-seeder.js",
    "dummy-users:delete": "node dist/config/dummy-users-seeder.js -d",
    "test": "ts-mocha --require ts-node/register  __tests__/*.test.ts",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Eduard Koshkelyan",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "mongoose-to-swagger": "^1.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "puppeteer": "^19.7.4",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.0",
    "@types/node-cron": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2",
    "ts-mocha": "^10.0.0"
  }
}
