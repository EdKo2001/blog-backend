{
  "name": "blog-back",
  "version": "1.0.5",
  "description": "Blog Back-end part done on Node.js",
  "main": "app.js",
  "scripts": {
    "dev": "ts-node --files app.ts",
    "start": "node dist/app.js",
    "build": "tsc -p .",
    "lint": "eslint . --fix",
    "test:jest": "jest --config ./jest.config.json",
    "test:jest-watch": "jest --config ./jest.config.json --watch",
    "seeder": "node dist/config/seeder.js",
    "seeder:delete": "node dist/config/seeder.js -d",
    "dummy-data": "node dist/config/dummy-data-seeder.js",
    "dummy-data:delete": "node dist/config/dummy-data-seeder.js -d"
  },
  "author": "Eduard Koshkelyan",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.5",
    "mongoose-to-swagger": "^1.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
